{
  "expo": {
    "name": "Pyramid Push",
    "slug": "pyramid-push",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pyramid-push-pro.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/pyramid-push-pro.png",
      "resizeMode": "contain",
      "backgroundColor": "#09090b"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pyramidpush.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/pyramid-push-pro.png",
        "backgroundColor": "#09090b"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.pyramidpush.app"
    },
    "web": {
      "favicon": "./assets/pyramid-push-pro.png",
      "bundler": "metro",
      "output": "single",
      "backgroundColor": "#09090b"
    },
    "plugins": ["expo-audio"]
  }
}
